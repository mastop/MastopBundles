{% extends "::backend.html.twig" %}

{% block content %}
    <h1>Cadastrar um novo menu</h1>
    <form action="{{ path('_menu_admin_store') }}" method="post" {{form_enctype(form) }}>
        {{ form_widget(form) }}
        <input type="hidden" name="id" value="{{ id }}" />
        <input type="submit" value="Cadastrar"/>
    </form>
    <br />
    <a href="{{ path('_menu_admin')}}">Listar menus!</a>
{% endblock %}

{% set code = code(_self) %}